#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/tony/web-apps/shamba/node_modules/.pnpm/@sanity+server@2.35.3_sfoxds7t5ydpegc3knd667wn6m/node_modules/@sanity/server/bin/node_modules:/home/tony/web-apps/shamba/node_modules/.pnpm/@sanity+server@2.35.3_sfoxds7t5ydpegc3knd667wn6m/node_modules/@sanity/server/node_modules:/home/tony/web-apps/shamba/node_modules/.pnpm/@sanity+server@2.35.3_sfoxds7t5ydpegc3knd667wn6m/node_modules/@sanity/node_modules:/home/tony/web-apps/shamba/node_modules/.pnpm/@sanity+server@2.35.3_sfoxds7t5ydpegc3knd667wn6m/node_modules:/home/tony/web-apps/shamba/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/tony/web-apps/shamba/node_modules/.pnpm/@sanity+server@2.35.3_sfoxds7t5ydpegc3knd667wn6m/node_modules/@sanity/server/bin/node_modules:/home/tony/web-apps/shamba/node_modules/.pnpm/@sanity+server@2.35.3_sfoxds7t5ydpegc3knd667wn6m/node_modules/@sanity/server/node_modules:/home/tony/web-apps/shamba/node_modules/.pnpm/@sanity+server@2.35.3_sfoxds7t5ydpegc3knd667wn6m/node_modules/@sanity/node_modules:/home/tony/web-apps/shamba/node_modules/.pnpm/@sanity+server@2.35.3_sfoxds7t5ydpegc3knd667wn6m/node_modules:/home/tony/web-apps/shamba/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@sanity/server/bin/sanity-server.js" "$@"
else
  exec node  "$basedir/../@sanity/server/bin/sanity-server.js" "$@"
fi
